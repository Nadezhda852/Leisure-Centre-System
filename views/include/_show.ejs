    <% for(var i=0; i<athlets.length;i++) {%>
<tbody>   
    <tr>
        <td><%= i+1 %></td>
        <td><%= athlets[i].name %></td>
        <td><%= athlets[i].email %></td>
        <td><%= athlets[i].gender %></td>
        <td><%= athlets[i].status %></td>
        <td>
            <a href="/update-athlet?id=<%= athlets[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= athlets[i]._id %>>
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
</tbody> 
<% } %>